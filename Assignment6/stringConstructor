package assignments.Assignment6;

public class StringConstructor {
    public static void main(String[] args) {
        B b = new B("Hello ");
        B[] references =new B[2];
        references[0] = new B("Welcome to ");
        references[1] = new B("Zemosolabs ");

    }
}
class  B{
    B(String name){
        System.out.print(name);
    }
}

